 close = false;
 animate = 1;
}

var __number = player_saved_get_numer();
with (menu_main_create(oUIMainFrame)) { // 1 (Game Start: Select Saved Data)
 if (__number == 0) {
  menu_main_label_add("저장된 게임 정보가 없습니다."); // 0
  with (menu_main_add(oUIMainButton, btn_width, "새 게임 시작", ui_callback_newgame))
   data = 0;
 } else {
  for (var i = 0; i &lt; __number; i++) {
   var saved_s = string(player_saved_get_variable(i, GAMESAVED_SPECIES));
   var saved_j = string(player_saved_get_variable(i, GAMESAVED_JOB));
   var saved_r = string(player_saved_get_variable(i, GAMESAVED_TURN));
   var saved_t = string(player_saved_get_variable(i, GAMESAVED_TIME));
   var res = "[ " + saved_s + "|" + saved_j + "|" + saved_r + "|" + saved_t + " ]";

   menu_main_add(oUIMainButton, btn_width, res, -1);
  }

  if (__number &lt; PLAYER_SAVED_NUMBER_MAX) {
   with (menu_main_add(oUIMainButton, btn_width, "새 게임 시작", ui_callback_newgame))
    data = __number;
  }
 }
 with (menu_main_add(oUIMainButton, btn_width * 0.5, "이전", ui_callback_mainback))
  __y = by;
}

with (menu_main_create(oUIMainFrame)) { // 2 (Highscore)
 with (zui_create(width * 0.5, height * 0.5 - 40, oUIFrame)) {
  zui_set_size(width - 80, height - 140);
 }

 with (menu_main_add(oUIMainButton, btn_width * 0.5, "이전", ui_callback_mainback)) // 0
  __y = by;
}

with (menu_main_create(oUIMainFrame)) { // 3 (Game Help)

